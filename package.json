{
  "name": "@mayntri/react-sdk",
  "version": "1.0.21",
  "type": "module",
  "homepage": "http://mayntri.github.io/react-sdk",
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc"
  },
  "dependencies": {
    "@mayntri/sdk": "^1.0.4",
    "@tanstack/react-query": "4.22.0 || >=^4.*",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "babel-loader": "^9.1.2",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "4.22.0 || >=^4.*",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-sdk.es.js",
      "require": "./dist/react-sdk.umd.js"
    }
  },
  "main": "./dist/react-sdk.umd.js",
  "module": "./dist/react-sdk.es.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "@mayntri:react-sdk": "https://npm.pkg.github.com"
  }
}
